data<-read.csv("C:/Users/Julia/Documents/analises_sisyrinchium/palmifolium.csv" , sep=",", dec=",")

boxplot(n.de.ramif.sinfl~Especie,
        data=data,
        main="Different boxplots for each Specie",
        xlab="Number of branches of synflorescence",
        ylab="Number of branches of synflorescence",
        col=c("#BF064E","#21570E","#10A2A6","#940E0E","#DFA327", "#C9E265", "#F172A4", "#370062"),
        border="black")

