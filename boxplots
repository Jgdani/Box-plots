####### box plots #######
dat<-read.csv("C:/Users/Julia/Documents/analises_sisyrinchium/vaginatum.csv" , sep=",", dec=",")
data<- dat[,c(2:14)] 
rownames(data) <- dat$Species[1:291] 

head(data) # abrir conjunto de dados

boxplot(n.flor.ripidio~Especie,
        data=data,
        main="Different boxplots for each Specie",
        xlab="Number of flowers per ripidio",
        ylab="Number of flowers",
        col= c("#359900","#D60D00","#10A2A6","#1C4042","#DFA327", "#9B003C"),
        border="black")

boxplot(comp.pedunc~Especie,
        data=data,
        main="Different boxplots for each Specie",
        xlab="Peduncle length",
        ylab="Peduncle length",
        col= c("#359900","#D60D00","#10A2A6","#1C4042","#DFA327", "#9B003C"),
        border="black")

boxplot(comp.pedic~Especie,
        data=data,
        main="Different boxplots for each Specie",
        xlab="Pedicel length",
        ylab="Pedicel length",
        col= c("#359900","#D60D00","#10A2A6","#1C4042","#DFA327", "#9B003C"),
        border="black")

boxplot(comp.valv.intern~Especie,
        data=data,
        main="Different boxplots for each Specie",
        xlab="internal valve length",
        ylab="internal valve length",
        col= c("#359900","#D60D00","#10A2A6","#1C4042","#DFA327", "#9B003C"),
        border="black")

boxplot(comp.valv.extern~Especie,
        data=data,
        main="Different boxplots for each Specie",
        xlab="External valve length",
        ylab="External valve length",
        col= c("#359900","#D60D00","#10A2A6","#1C4042","#DFA327", "#9B003C"),
        border="black")

boxplot(comp.folha~Especie,
        data=data,
        main="Different boxplots for each Specie",
        xlab="Leaf length",
        ylab="Leaf length",
        col= c("#359900","#D60D00","#10A2A6","#1C4042","#DFA327", "#9B003C"),
        border="black")

boxplot(comp.bainha~Especie,
        data=data,
        main="Different boxplots for each Specie",
        xlab="Leaf sheath length",
        ylab="Leaf sheath length",
        col= c("#359900","#D60D00","#10A2A6","#1C4042","#DFA327", "#9B003C"),
        border="black")

boxplot(larg.valv.intern~Especie,
        data=data,
